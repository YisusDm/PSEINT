Proceso sin_titulo
	Definir A,b,c,d,e Como Real;
	Escribir "Ingrese un angulo";
	leer A;
	b<-a*3.14/180;
	c <- sen(b);
	
	b<-a*3.14/180;
	d <- cos(b);
	
	b<-a*3.14/180;
	e <- tan(b);
	
	
	
	
	
